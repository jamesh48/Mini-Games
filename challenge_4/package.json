{
  "name": "beatminesweeper.app",
  "version": "1.0.0",
  "description": "One Minesweeper to rule them all",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "start": "nodemon dist/server/server.js",
    "build": "grunt webpack",
    "database": "npx sequelize db:drop && npx sequelize db:create && npx sequelize db:migrate",

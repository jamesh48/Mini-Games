{
  "name": "beatminesweeper.app",
  "version": "1.0.0",
  "description": "One Minesweeper to rule them all",
  "main": "ts-dist/server/server.js",
  "scripts": {
    "test": "jest",
    "build": "webpack --config configs/webpack/webpack.config.js",
    "webpack:test": "webpack t configs/webpack/webpack.config.js",
    "create:database": "npx mikro-orm schema:create --run",
    "drop:database": "npx mikro-orm schema:drop --drop-db --run",
    "create:migration": "npx mikro-orm migration:create",
    "dev:build": "webpack --config configs/webpack/webpack.config.js && concurrently \"webpack --config configs/webpack/webpack.config.js --watch \" \"npm run dev:start\"",
    "dev:start": "nodemon ts-dist/server/server.js",
    "grunt:uglify": "grunt uglify --gruntfile configs/grunt/Gruntfile.js",
    "grunt:cssmin": "grunt cssmin --gruntfile configs/grunt/Gruntfile.js",
    "grunt:bucketDeploy": "grunt bucketDeploy --gruntfile configs/grunt/GruntFile.js",
    "deploy": "grunt deploy-all --gruntfile configs/grunt/Gruntfile.js",
    "postdeploy": "pm2 deploy configs/pm2/ecosystem.config.js production",
    "postinstall": "npm link typescript",
    "gen": "graphql-codegen --config configs/codegen/codegen.yml"
  },
  "keywords": [
    "Minesweeper",
    "beatminesweeper.app"
  ],
  "author": "James Hrivnak",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.4.15",
    "@mikro-orm/cli": "^4.5.9",
    "@mikro-orm/core": "^4.5.9",
    "@mikro-orm/migrations": "^4.5.9",
    "@mikro-orm/postgresql": "^4.5.9",
    "apollo-server-express": "^3.3.0",
    "argon2": "^0.28.2",
    "axios": "^0.21.1",
    "colors": "^1.4.0",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.2",
    "formik": "^2.2.9",
    "graphql": "^15.6.0",
    "graphql-sequelize": "^9.4.3",
    "nodemon": "^2.0.12",
    "pg": "^8.6.0",
    "postgres": "^1.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.9",
    "sequelize": "^6.6.5",
    "ts-node": "^10.2.1",
    "webpack-merge": "^5.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-operations": "2.1.5",
    "@graphql-codegen/typescript-react-apollo": "3.1.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/connect-redis": "0.0.17",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "1.17.0",
    "@types/node": "^16.10.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.0",
    "@types/redis": "^2.8.32",
    "babel-jest": "^27.0.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babelify": "^10.0.0",
    "concurrently": "^6.2.1",
    "css-loader": "^5.2.6",
    "enzyme": "^3.11.0",
    "file-loader": "^6.2.0",
    "global-jsdom": "^8.1.0",
    "grunt": "^1.4.1",
    "grunt-aws-s3": "^2.0.2",
    "grunt-cloudfront-invalidate": "^1.0.1",
    "grunt-contrib-cssmin": "^4.0.0",
    "grunt-contrib-uglify": "^5.0.1",
    "grunt-git": "^1.1.1",
    "grunt-webpack": "^4.0.3",
    "jest": "^27.2.2",
    "jsdom": "^17.0.0",
    "jsdom-global": "^3.0.2",
    "mini-css-extract-plugin": "^2.2.0",
    "node-sass": "^6.0.1",
    "react-test-renderer": "^17.0.2",
    "reactify": "^1.1.1",
    "sass-loader": "^12.1.0",
    "sequelize-cli": "^6.2.0",
    "style-loader": "^3.0.0",
    "time-grunt": "^2.0.0",
    "ts-loader": "^9.2.6",
    "tslint": "^6.1.3",
    "type-graphql": "^1.1.1",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0"
  },
  "mikro-orm": {
    "useTsNode": true,
    "tsConfigPath": "./configs/mikro-orm/tsconfig.mikro-orm-json.js",
    "configPaths": [
      "./ts-src/database/mikro-orm.config.ts",
      "./ts-dist/database/mikro-orm.config.js"
    ]
  }
}
